booleanbusqueda ( vector a , int z int d, elem x){


int mitad;

if (z>d)
  return(False);
 
else {
    mitad =(z+d)/2;
    if(a [mitad]==x)
      Return(True);
     else 
     
      if( a[mitad]>x)
        return (busqueda(a,z,mitad-1,x));
        else
          return(busqueda(a, mitad+1,d,x));
          }

}


